# Cadabra Server Configuration

# Server Port
# Default: 6942
PORT=6942

# Server Host
# Use 0.0.0.0 to accept connections from any interface
# Use 127.0.0.1 for localhost only
# Default: 0.0.0.0
HOST=0.0.0.0

# Logging Level
# Options: debug, info, warn, error
# Default: info
LOG_LEVEL=info

# CORS (Cross-Origin Resource Sharing)
# Set to false to disable CORS (not recommended for most deployments)
# Default: true
CORS_ENABLED=true
