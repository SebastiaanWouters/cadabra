{
  "name": "@sebastiaanwouters/cadabra",
  "version": "0.4.1",
  "description": "Intelligent query cache library - SQL parser, cache key generation, and invalidation analysis. Server binaries available via GitHub Releases.",
  "type": "module",
  "module": "index.ts",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./index.ts"
    },
    "./server": {
      "import": "./server.ts",
      "types": "./server.ts"
    }
  },
  "files": [
    "cadabra.ts",
    "index.ts",
    "server.ts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "cache",
    "query-cache",
    "sql",
    "database",
    "invalidation",
    "sql-parser",
    "bun",
    "typescript"
  ],
  "author": "Cadabra Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastiaanWouters/cadabra.git",
    "directory": "packages/cadabra"
  },
  "bugs": {
    "url": "https://github.com/SebastiaanWouters/cadabra/issues"
  },
  "homepage": "https://github.com/SebastiaanWouters/cadabra/tree/main/packages/cadabra#readme",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "node-sql-parser": "^5.3.13"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "test": "bun test",
    "start": "bun run server.ts",
    "dev": "bun --hot run server.ts"
  },
  "publishConfig": {
    "access": "public"
  }
}
